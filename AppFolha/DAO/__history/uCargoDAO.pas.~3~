unit uCargoDAO;

interface

uses uConexao;

type
  TCargoDAO = Class
    private
      FConexao: TConexao;

    public
      constructor Create;
      destructor Destroy; override;
  end;

implementation

{ TCargoDAO }

constructor TCargoDAO.Create;
begin
  FConexao := TConexao.Create;
end;

destructor TCargoDAO.Destroy;
begin
  inherited Destroy;
end;

end.
